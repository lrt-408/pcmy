   "source": [
    "def print_christmas_tree(n):\n",
    "    # 打印圣诞树\n",
    "    for i in range(n):\n",
    "        print(\" \" * (n - i - 1) + \"*\" * (2 * i + 1))\n",
    "    \n",
    "    # 打印树干\n",
    "    print(\" \" * (n - 1) + \"|\")\n",
    "\n",
    "# 调用函数打印圣诞树\n",
    "n = 9  # 圣诞树的行数\n",
    "print_christmas_tree(n)"
   ]
  }
